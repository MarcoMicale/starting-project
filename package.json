{
  "name": "starting-project",
  "version": "0.0.2",
  "description": "A solid 11ty starting point to speed up the creation process",
  "main": "README.md",
  "scripts": {
    "go!": "npx @11ty/eleventy --incremental",
    "dev": "rimraf public && npx @11ty/eleventy --serve --watch",
    "instal": "npm install && npm install -g npm-check-updates",
    "upgrade": "ncu --upgrade && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarcoMicale/starting-project.git"
  },
  "keywords": [
    "11ty",
    "css",
    "md",
    "html"
  ],
  "author": "Marco Micale",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/MarcoMicale/starting-project/issues"
  },
  "homepage": "https://github.com/MarcoMicale/starting-project#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "eleventy-plugin-pwa-v2": "^1.0.1",
    "html-minifier": "^4.0.0",
    "markdown-it": "^14.0.0",
    "markdown-it-attrs": "^4.1.6",
    "rimraf": "^5.0.5"
  }
}
